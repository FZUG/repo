+	# symlinks are next
+	cat /${TMP_DIR}/newfiles.tmp | egrep -v '^[-0-9][0-9]*[[:space:]]*(unlink|access)' | cut -f 4 | egrep -v "^(/dev|$BASE_TMP_DIR|/tmp)" | grep -v "#success" | sort -u  &gt;&gt; /${TMP_DIR}/newfiles
+        # Create another list of modified files that exclude all files the
