-# Output all the parents of the files given as arguments to stdout. Remove
-# duplicates.
-function list_parents {
-    src="$*"
-    files=`for f in $src; do
-    echo "$f" | awk '
-    BEGIN{
-        FS = "/"
-    }
-    {
-        for (i=1;i&lt;NF;i++){
-            for (j=1;j&lt;=i;j++){
-                printf "%s",$(j)
-                if (j != i){
-                        printf "/"
-                    }
-            }
-            printf " "
-        }
-    }'
-    done`
-
-    echo "${files}"  | tr ' ' '\n' | uniq | tr '\n' ' '
-    return $?
-}
