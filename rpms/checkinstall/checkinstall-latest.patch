-function followlinks () {
-    local entry="$1"
-    local symlink
-
-    case "$entry" in /*) true ;;
-        *) entry="`pwd`/$entry";;
-    esac
-    if [ -L "$entry" ]; then
-        symlink=`ls -l "$entry" | sed -e 's/.* -&gt; //'`
-        
-        case "$symlink" in /*) entry="$symlink";;
-            *) entry=`dirname "$entry"`/$symlink;;
