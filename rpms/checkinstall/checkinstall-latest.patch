+
+    read YN
+    YN=`echo $YN | tr 'A-Z' 'a-z'`
+    ! [ "$YN" ] &amp;&amp; YN=$DEFAULTYN          # If the user pressed ENTER then 
+
+else
+   YN=$DEFAULTYN
+   echo $YN
+fi
+
+    if [ "$YN" = "y" ] ;then    # We return something useful for a 
+       return 0                 # simpler sintax ahead (12/dic/2000-Izto)
+    else
+       return 1
+    fi
+
+
+}
